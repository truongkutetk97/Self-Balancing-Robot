.................... dow  = bcd2bin(i2c_read() & 0x7f);   // REG 3 
.................... day  = bcd2bin(i2c_read() & 0x3f);   // REG 4 
.................... mth  = bcd2bin(i2c_read() & 0x1f);   // REG 5 
.................... year = bcd2bin(i2c_read(0));         // REG 6 
.................... i2c_stop(); 
