.................... i2c_write(bin2bcd(mth));      // REG 5 
.................... i2c_write(bin2bcd(year));     // REG 6 
.................... i2c_write(0x90);              // REG 7  1Hz squarewave output pin 
.................... i2c_stop(); 
.................... } 
