.................... i2c_write(0xD0);      // WR to RTC 
.................... i2c_write(0x00);      // REG 0 
.................... i2c_start(); 
.................... i2c_write(0xD1);      // RD from RTC 
.................... initsec  = bcd2bin(i2c_read() & 0x7f); 
.................... initmin  = bcd2bin(i2c_read() & 0x7f); 
.................... inithr   = bcd2bin(i2c_read() & 0x3f); 
.................... initdow  = bcd2bin(i2c_read() & 0x7f);   // REG 3 
